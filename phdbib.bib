issn = {18737544},
keywords = {Aging,Alzheimer's disease,Biomarker,CA1,Hippocampus,Magnetic resonance imaging},
mendeley-groups = {PHD/Shape Analysis,Papers bib/hipppaper/subfields,PhDThesis/Hippo},
month = {nov},
