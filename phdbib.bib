arxivId = {NIHMS150003},
author = {Heimann, Tobias and Meinzer, Hans Peter},
doi = {10.1016/j.media.2009.05.004},
eprint = {NIHMS150003},
isbn = {1361-8415},
issn = {13618415},
journal = {Med. Image Anal.},
keywords = {Active Appearance model,Active Shape model,Deformable surface,Statistical shape model},
