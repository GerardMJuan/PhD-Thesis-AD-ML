number = {4},
pages = {1385--1401},
pmid = {27079727},
title = {{Prediction of Alzheimer's Disease in Amnestic Mild Cognitive Impairment Subtypes: Stratification Based on Imaging Biomarkers.}},
volume = {52},
year = {2016}
}
